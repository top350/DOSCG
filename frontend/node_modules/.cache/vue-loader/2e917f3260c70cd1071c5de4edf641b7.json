    return this.url+'&origin='+this.origin+'&destination='+this.destination\n    },\n    getResponse:async function(){\n        var BCStorage = Vue.localStorage.get('BC')\n        if(BCStorage==null){\n        try {\n       const res = await axios.get('http://localhost:3000/findBC');\n      this.$localStorage.set('BC', JSON.stringify(res.data))\n      var data =JSON.parse(this.$localStorage.get('BC'))\n      console.log(data)\n      this.B =data.B\n      this.C =data.C\n     \n    } catch (error) {\n      console.log(error)\n    }\n  }else{\n     console.log('local storage has been stored')\n     var data2 = this.$localStorage.get('BC')\n     this.B =data2.B\n    this.C =data2.C\n  }\n    }\n}\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style src='../styles/findBC.css' scoped>\n\n\n</style>\n"]}]}